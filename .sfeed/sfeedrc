filter() {
	case "$1" in
	"NakeyJakey" | "Gerbert Johnson" | "Gatsu")
		cat;;
	*)
		cat | \
		awk -F '\t' 'BEGIN { OFS = "\t"; }
		{
			$3 = $8; # link
			print $0;
		}'
		;;
    esac
}
