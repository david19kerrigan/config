"$HOME/.local/bin/raise_volume"
    XF86AudioRaiseVolume	

"$HOME/.local/bin/lower_volume"
    XF86AudioLowerVolume

"exec mpc toggle"
    Shift + Alt + K

"mpc next ; mpc_status"
    Shift + Alt + L

"mpc prev ; mpc_status"
    Shift + Alt + J

"exec play_album"
    Shift + Alt + A

"exec play_song"
    Shift + Alt + S

"exec play_podcast"
    Shift + Alt + P

"exec mpc_status"
    Shift + Alt + M

"sudo zzz"
    Shift + Alt + Control + S

"sudo poweroff"
    Shift + Alt + Control + P

"sudo reboot"
    Shift + Alt + Control + R

"/home/david/.local/bin/omenu"
    Shift + Alt + B

"/home/david/.local/bin/pmenu"
    Shift + Alt + N

"exec brightnessctl set 10%+"
    XF86MonBrightnessUp

"exec brightnessctl set 10%-"
    XF86MonBrightnessDown

