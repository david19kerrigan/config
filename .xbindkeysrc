"pactl set-sink-volume @DEFAULT_SINK@ +5%"
    XF86AudioRaiseVolume	

"pactl set-sink-volume @DEFAULT_SINK@ -5%"
    XF86AudioLowerVolume

"exec mpc toggle ; playerctl play-pause"
    Shift + Alt + K

"exec mpc next ; playerctl next"
    Shift + Alt + L

"exec mpc prev ; playerctl previous"
    Shift + Alt + J

"loginctl suspend"
    Shift + Alt + Control + S

"loginctl poweroff"
    Shift + Alt + Control + P

"loginctl reboot"
    Shift + Alt + Control + R

"/home/david/.local/bin/omenu"
    Shift + Alt + N

"/home/david/.local/bin/pmenu"
    Shift + Alt + M

"exec brightnessctl set 10%+"
    XF86MonBrightnessUp

"exec brightnessctl set 10%-"
    XF86MonBrightnessDown

