"$HOME/.local/bin/raise_volume"
    XF86AudioRaiseVolume	

"$HOME/.local/bin/lower_volume"
    XF86AudioLowerVolume

"exec mpc toggle"
    Shift + Alt + K

"exec mpc next"
    Shift + Alt + L

"exec mpc prev"
    Shift + Alt + J

"sudo zzz"
    Shift + Alt + Control + S

"sudo poweroff"
    Shift + Alt + Control + P

"sudo reboot"
    Shift + Alt + Control + R

"/home/david/.local/bin/omenu"
    Shift + Alt + N

"/home/david/.local/bin/pmenu"
    Shift + Alt + M

"exec brightnessctl set 10%+"
    XF86MonBrightnessUp

"exec brightnessctl set 10%-"
    XF86MonBrightnessDown

